{
  "mcpServers": {
    "laravel-boost": {
      "command": "vendor/bin/sail",
      "args": [
        "artisan",
        "boost:mcp"
      ]
    },
    "storybook-mcp": {
      "type": "http",
      "url": "http://localhost:6006/mcp"
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    }
  }
}
